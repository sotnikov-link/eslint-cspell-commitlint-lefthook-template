{
  "name": "yarn-workspaces-eslint-cspell-commitlint-lefthook-template",
  "type": "module",
  "packageManager": "yarn@4.3.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "check-commitlint": "commitlint --from 35bf7118",
    "check-eslint": "eslint --color --cache --max-warnings=0 --report-unused-disable-directives",
    "check-eslint-all": "yarn check-eslint ./",
    "check-prettier": "prettier --check",
    "check-prettier-all": "yarn check-prettier .",
    "check-cspell": "cspell --quiet --cache",
    "check-cspell-all": "yarn check-cspell ."
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.23.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3",
    "@cspell/dict-npm": "^5.0.16",
    "@eslint-react/eslint-plugin": "^1.5.27",
    "cspell": "^8.10.4",
    "eslint": "9.5.0",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.8",
    "lefthook": "^1.6.18",
    "prettier": "^3.3.2",
    "typescript": "^5.5.2"
  }
}
