{
  "name": "yarn-workspaces-eslint-cspell-commitlint-lefthook-template",
  "type": "module",
  "packageManager": "yarn@4.3.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "check-commitlint": "commitlint --from 35bf7118",
    "check-eslint": "eslint --color --cache --max-warnings=0 --report-unused-disable-directives",
    "check-eslint-all": "yarn check-eslint ./",
    "check-prettier": "prettier --check",
    "check-prettier-all": "yarn check-prettier .",
    "check-cspell": "cspell --quiet --cache",
    "check-cspell-all": "yarn check-cspell ."
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3",
    "@eslint/js": "^9.6.0",
    "@types/eslint__js": "^8.42.3",
    "cspell": "^8.9.1",
    "eslint": "^9.6.0",
    "eslint-config-canonical": "^43.0.13",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-regex": "^1.10.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "lefthook": "^1.6.18",
    "prettier": "^3.3.2",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.14.1"
  }
}
